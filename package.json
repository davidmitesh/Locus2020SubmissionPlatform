    "babel-preset-stage-1": "^6.1.18",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "lodash": "^3.10.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-images-upload": "^1.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.1.2",
    "redux": "4.0.0",
